<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	xmlns:options="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options"
	xmlns:themes="clr-namespace:FolderBrowser.Themes"
	mc:Ignorable="d">
	<ResourceDictionary.MergedDictionaries>
		<ResourceDictionary Source="/FolderBrowser;component/Images/MetroLightIcons.xaml" />
		<!--  ResourceDictionary Source="/UserNotification;component/Themes/LightBrushs.xaml" /  -->
		<ResourceDictionary Source="/InplaceEditBoxLib;component/Themes/LightBrushs.xaml" />
	</ResourceDictionary.MergedDictionaries>

	<SolidColorBrush
		x:Key="{x:Static themes:ResourceKeys.DropDownBackgroundKey}"
		options:Freeze="true"
		Color="White" />
	<SolidColorBrush
		x:Key="{x:Static themes:ResourceKeys.DialogBackgroundKey}"
		options:Freeze="true"
		Color="White" />

	<SolidColorBrush
		x:Key="{x:Static themes:ResourceKeys.TreeViewBackgroundKey}"
		options:Freeze="true"
		Color="White" />

	<SolidColorBrush
		x:Key="{x:Static themes:ResourceKeys.DialogForegroundKey}"
		options:Freeze="true"
		Color="#FF000000" />

</ResourceDictionary>
